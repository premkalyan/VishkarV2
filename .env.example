# VishkarV2 Configuration Template
# Copy this to .env and fill in your values

# AI Provider Configuration
OPENROUTER_API_KEY=sk-or-v1-xxx
ANTHROPIC_API_KEY=sk-ant-xxx
OPENAI_API_KEY=sk-xxx

# Goose AI Agent Configuration
GOOSE_PROVIDER=openrouter
GOOSE_MODEL=x-ai/grok-4-fast:free
GOOSE_API_KEY=${OPENROUTER_API_KEY}

# JIRA Configuration
JIRA_HOST=https://your-instance.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-jira-api-token
JIRA_PROJECT_KEY=V2

# Confluence Configuration
CONFLUENCE_URL=https://your-instance.atlassian.net/wiki
CONFLUENCE_EMAIL=your-email@example.com
CONFLUENCE_API_TOKEN=your-confluence-api-token
CONFLUENCE_SPACE_KEY=V2

# GitHub Configuration
GITHUB_TOKEN=ghp_xxx
GITHUB_ORG=

# Project Registry
PROJECT_REGISTRY_TOKEN=pk_xxx
